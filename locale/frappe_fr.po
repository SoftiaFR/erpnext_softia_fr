"{% if address_line2 %}{ address_line2 }}&lt;br&gt;{% endif -%}\n"
"{{ city }}&lt;br&gt ;\n"
"{% if state %}{{{ state }}&lt;br&gt;{% endif -%}\n"
"{% if pincode %} PIN : {{ pincode }}&lt;br&gt;{% endif -%}\n"
"{{ pays }}&lt;br&gt ;\n"
"{% if phone %}Phone : {{ phone }}&lt;br&gt;{% endif -%}\n"
"{% if fax %}Fax : {{ fax }}&lt;br&gt;{% endif -%}\n"
